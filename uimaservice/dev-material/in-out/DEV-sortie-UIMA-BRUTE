    <j.0:coveredText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a</j.0:coveredText>
    <j.0:uimaType rdf:datatype="http://www.w3.org/2001/XMLSchema#string">org.apache.uima.TokenAnnotation</j.0:uimaType>
    <j.0:hasFeature rdf:resource="http://clerezza.apache.org/2010/22/uima-entities#Feature821708"/>
