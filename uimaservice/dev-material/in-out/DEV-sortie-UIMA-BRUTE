  <rdf:Description rdf:about="http://clerezza.apache.org/2010/22/uima-entities#Feature1441530">
    <j.0:featureName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">uima.tcas.Annotation:begin</j.0:featureName>
